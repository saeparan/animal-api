// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Animals {
  id                 String    @id @default(auto()) @map("_id") @db.ObjectId
  desertionNo        Int       @unique
  noticeNo           String    @unique
  happenDt           DateTime?
  happenPlace        String?
  kindCd             String?
  kindCdDetail       String?
  colorCd            String?
  age                Int?
  weight             Float?
  noticeStartDate    DateTime?
  noticeEndDate      DateTime?
  fileName           String?
  popFile            String?
  processState       String?
  processStateReason String?
  sex                String?
  neuter             String?
  specialText        String?
  careName           String?
  officeTel          String?
  careTel            String?
  careAddress        String?
  orgNm              String?
  chargeNm           String?
}
